<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="BOTTOM_CENTER" prefHeight="318.0" prefWidth="362.0" spacing="24.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cronotimer.FXMLCronoController">
   <children>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
              <Label fx:id="cronoText" minHeight="16" minWidth="69" text="00:00:00" textFill="#215be1">
               <font>
                  <Font size="53.0" />
               </font>
            </Label>
         </children>
      </StackPane>
      <HBox alignment="CENTER" spacing="12.0">
         <children>
              <Button fx:id="butStart" onAction="#iniciar" text="start" />
            <Button fx:id="butStop" onAction="#parar" text="stop" />
            <Button fx:id="butReset" onAction="#reset" text="reset" />
            <CheckBox fx:id="coundown" mnemonicParsing="false" onAction="#cuentaatras" text="Countdown" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
