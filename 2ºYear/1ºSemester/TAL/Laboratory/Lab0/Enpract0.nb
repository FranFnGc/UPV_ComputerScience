Cell[TextData[StyleBox["Algorithm: Initialize a counter to zero. Traverse the \
list and compare each element with the input one. Increase the counter in \
case both are equal.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, 
   3.5872819182482033`*^9, {3.587795849779948*^9, 
   3.5877959129150743`*^9}},ExpressionUUID->"d542e682-92f1-4c94-b0dd-\
c9ffefa48abe"],

Cell[BoxData[
 RowBox[{" ", 
  RowBox[{
   RowBox[{"Occurrences", "[", 
    RowBox[{"l_", ",", "e_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"counter", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"counter", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "\[LessEqual]", " ", 
         RowBox[{"Length", "[", "l", "]"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"l", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", "e"}], ",", 
           RowBox[{"counter", "++"}]}], "]"}], ";"}]}], "\[IndentingNewLine]",
        "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "counter", "]"}], ";"}]}], "\[IndentingNewLine]",
     "]"}]}]}]], "Input",
 CellChangeTimes->{{3.745908946603879*^9, 3.7459090836179237`*^9}, {
  3.7459091169945884`*^9, 3.7459092771924305`*^9}, {3.745909311708537*^9, 
