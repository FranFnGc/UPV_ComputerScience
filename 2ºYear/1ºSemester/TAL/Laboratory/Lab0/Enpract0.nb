Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module whose input is a word x and an integer k. The module should return \
the set of k-length segments of x. "
}], "ItemNumbered",
 CellChangeTimes->{
  3.587281372564268*^9, {3.58728148594456*^9, 3.587281550798246*^9}, {
   3.587283510258602*^9, 3.587283510357462*^9}, {3.587796072665926*^9, 
   3.587796153058509*^9}},ExpressionUUID->"3b322026-90a2-415c-9b59-\
3feed0f4540c"],

Cell[TextData[StyleBox["Hint: The last position of x where it is possible to \
find a segment of length k is position n-k+1, where n denotes the length of \
the word.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, {
  3.587281707489728*^9, 3.587281913367154*^9}, {3.5877961549608593`*^9, 
  3.587796219659649*^9}},ExpressionUUID->"74f72bba-21d2-4ff1-bfce-\
1e8b59359755"],

Cell[BoxData[
 RowBox[{" ", 
  RowBox[{
   RowBox[{
    RowBox[{"Segments", "[", 
     RowBox[{"x_", ",", "k_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"l", ",", "n", ",", "aux"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"l", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"aux", "=", "x"}], ";", "\[IndentingNewLine]", 
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "x", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "\[LessEqual]", " ", 
          RowBox[{"n", "-", "k", "+", "1"}]}], ",", 
         RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"l", ",", 
            RowBox[{"Take", "[", 
             RowBox[{"aux", ",", "k"}], "]"}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"aux", " ", "=", 
           RowBox[{"Rest", "[", "aux", "]"}]}], ";"}]}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "l", "]"}], ";"}]}], "\[IndentingNewLine]", 
     "]"}]}], " ", "\[IndentingNewLine]", " ", 
   RowBox[{
    RowBox[{"Segment", "[", 
     RowBox[{"x_", ",", "k_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"l", ",", "n"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"l", "=", 
        RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "x", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "\[LessEqual]", " ", 
          RowBox[{"n", "-", "k", "+", "1"}]}], ",", 
         RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"l", ",", 
            RowBox[{"Take", "[", 
             RowBox[{"x", ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"i", "+", "k", "-", "1"}]}], "}"}]}], "]"}]}], "]"}], 
          ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "l", "]"}], ";"}]}], "\[IndentingNewLine]", 
     "]"}], " "}]}]}]], "Input",
 CellChangeTimes->{
  3.745910416869316*^9, {3.7459105083700304`*^9, 3.7459105474276905`*^9}, {
   3.7459105944362736`*^9, 3.7459106643186536`*^9}, {3.7459106943644114`*^9, 
   3.745910722795182*^9}, {3.745910767383777*^9, 3.7459107911926126`*^9}, {
