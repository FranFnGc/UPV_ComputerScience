    3.59075818353125*^9}, {3.590758256296875*^9, 3.59075825753125*^9}, {
    3.59075934903125*^9, 3.59075935453125*^9}, {3.590759429859375*^9, 
    3.590759491828125*^9}, {3.590759774734375*^9, 3.59075983878125*^9}, {
    3.590759888609375*^9, 3.5907599173125*^9}, {3.590760031953125*^9, 
    3.590760032890625*^9}, {3.59076011375*^9, 3.590760155234375*^9}, {
    3.590760914765625*^9, 3.590760917953125*^9}, {3.590762431890625*^9, 
    3.590762534*^9}, 3.59197785975*^9},ExpressionUUID->
  "d9562a9e-b8eb-4a67-a3a6-10fce4f31814"]
}], "Text",
