Cell[TextData[{
 "To integrate in Mathematica we use \[OpenCurlyDoubleQuote]Integrate[]\
\[CloseCurlyDoubleQuote]. Here is how it works this function:\n\t\
Integrate[f,x] ----------->the indefinite integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{"f", 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "dc6b2631-feb8-4d8b-bece-cfb9a6efbf49"],
 "\n\tIntegrate[f,x,y] ----------->the multiple integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{"f", 
     RowBox[{"\[DifferentialD]", "x"}], " ", 
     RowBox[{"\[DifferentialD]", "y"}]}]}], TraditionalForm]],ExpressionUUID->
  "cef6fad8-904a-489c-9245-4eae50a8c4d7"],
 "\n\tIntegrate[f,{x,x min, x max}] ----------->the definite integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"x", " ", "min"}], 
     RowBox[{"x", " ", "max"}]], 
    RowBox[{"f", 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "aea54f9b-1802-4d8b-8876-d382c1421725"],
 "\n\tTo do aproximated integrals:\n\t\t",
 Cell[BoxData[{
  RowBox[{
   RowBox[{
    RowBox[{"Trapezoid", "[", 
     RowBox[{"a_", ",", "b_", ",", "n_", ",", "d_", ",", "f_"}], "]"}], ":=", 
    RowBox[{"N", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"f", "[", "a", "]"}], "+", 
         RowBox[{"f", "[", "b", "]"}], "+", 
         RowBox[{"2", " ", 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"k", "=", "1"}], 
            RowBox[{"n", "-", "1"}]], 
           RowBox[{"f", "[", 
            RowBox[{"a", "+", 
             RowBox[{"k", " ", 
              RowBox[{"(", 
               FractionBox[
                RowBox[{"b", "-", "a"}], "n"], ")"}]}]}], "]"}]}]}]}], ")"}], 
       RowBox[{"(", 
        FractionBox[
         RowBox[{"b", "-", "a"}], 
         RowBox[{"2", "  ", "n"}]], ")"}]}], ",", "d"}], "]"}]}], 
   ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ErrorTrapezoid", "[", 
     RowBox[{"a_", ",", "b_", ",", "n_", ",", "M2_"}], "]"}], ":=", " ", 
    RowBox[{
     FractionBox[
      RowBox[{" ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"b", "-", "a"}], ")"}], "3"]}], 
      RowBox[{"12", " ", 
       SuperscriptBox["n", "2"]}]], "M2"}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Simpson", "[", 
     RowBox[{"a_", ",", "b_", ",", "n_", ",", "d_", ",", "f_"}], "]"}], ":=", 
    RowBox[{"N", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"f", "[", "a", "]"}], "+", 
         RowBox[{"f", "[", "b", "]"}], "+", 
         RowBox[{"4", 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"k", "=", "0"}], 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"n", "/", "2"}], ")"}], "-", "1"}]], 
           RowBox[{"f", "[", 
            RowBox[{"a", "+", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"2", "  ", "k"}], " ", "+", " ", "1"}], ")"}], 
              RowBox[{"(", 
               FractionBox[
                RowBox[{"b", "-", "a"}], "n"], ")"}]}]}], "]"}]}]}], "+", 
         RowBox[{"2", " ", 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"k", "=", "1"}], 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"n", "/", "2"}], ")"}], "-", "1"}]], 
           RowBox[{"f", "[", 
            RowBox[{"a", "+", 
             RowBox[{"2", "  ", "k", " ", 
              RowBox[{"(", 
               FractionBox[
                RowBox[{"b", "-", "a"}], "n"], ")"}]}]}], " ", "]"}]}]}]}], 
        ")"}], 
       RowBox[{"(", 
        FractionBox[
         RowBox[{"b", "-", "a"}], 
         RowBox[{"3", " ", "n"}]], ")"}]}], ",", "d"}], "]"}]}], 
   ";"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ErrorSimpson", "[", 
     RowBox[{"a_", ",", "b_", ",", "n_", ",", "M4_"}], "]"}], ":=", " ", 
    RowBox[{
     FractionBox[
      RowBox[{" ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"b", "-", "a"}], ")"}], "5"]}], 
      RowBox[{"180", 
       SuperscriptBox["n", "4"]}]], "M4"}]}], ";"}]}],
  CellChangeTimes->{{3.5906761119375*^9, 3.59067614209375*^9}, {
    3.59067619465625*^9, 3.5906762316875*^9}, 3.590676928140625*^9, {
