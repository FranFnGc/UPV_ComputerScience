Cell[BoxData[
 RowBox[{
  RowBox[{"MyPower", "[", 
   RowBox[{"base_", ",", "exponent_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", "result"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"result", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "exponent"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"result", "=", 
         RowBox[{"result", "*", "base"}]}], ";"}]}], "\[IndentingNewLine]", 
      "]"}], ";", 
     RowBox[{"(*", " ", "for", " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "result", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}], 
  RowBox[{"(*", " ", "MyPower", " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.5877953150010138`*^9, 
  3.5877953558717327`*^9}},ExpressionUUID->"31a4f938-9e86-4540-a573-\
e4dc4f27468f"],

Cell["\<\
Once the module is defined and evaluated, it can be called, as the main \
