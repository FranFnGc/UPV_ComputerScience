Cell[TextData[StyleBox["Algorithm: Initialize a counter to zero. Traverse the \
list and compare each element with the input one. Increase the counter in \
case both are equal.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, 
   3.5872819182482033`*^9, {3.587795849779948*^9, 
   3.5877959129150743`*^9}},ExpressionUUID->"41d88321-c47f-4f5b-ba5c-\
37078678d19f"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"OccurrencesOf", "[", 
    RowBox[{"list_", ",", " ", "x_"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"c", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"c", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", " ", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<", 
         RowBox[{"Length", "[", "list", "]"}]}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"list", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", "x"}], ",", 
           RowBox[{
            RowBox[{"c", "++"}], ";"}]}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "c", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6521648788248854`*^9, 3.6521650021554003`*^9}, {
  3.6521650347694902`*^9, 3.652165059592068*^9}, {3.6521651316781073`*^9, 
