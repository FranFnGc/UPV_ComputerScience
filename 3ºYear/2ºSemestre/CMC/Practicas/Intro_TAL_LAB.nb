Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PrefixesOfWord", "[", "word_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"list", ",", " ", "i"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"list", " ", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", 
         RowBox[{"StringLength", "[", "word", "]"}]}], ",", 
        RowBox[{"i", "\[GreaterEqual]", " ", "0"}], ",", 
        RowBox[{"i", "--"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"list", ",", " ", 
           RowBox[{"StringDrop", "[", 
            RowBox[{"word", ",", 
             RowBox[{"-", "i"}]}], "]"}]}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "list", "]"}], ";"}]}], "\[IndentingNewLine]", 
